[
    {
        "selector": "Y",
        "eval": "timestamp.getFullYear()",
        "desc": "Full year (e.g 2020)"
    },
    {
        "selector": "y",
        "eval": "timestamp.getYear() - 100",
        "desc": "Last 2 digits of year (e.g 20)"
    },
    {
        "selector": "W",
        "eval": "['Sunday', 'Monday', 'Tuesday', 'Wednesday', 'Thursday', 'Friday', 'Saturday'][timestamp.getDay()]",
        "desc": "Day of the week (e.g Wednesday)"
    },
    {
        "selector": "w",
        "eval": "['Sunday', 'Monday', 'Tuesday', 'Wednesday', 'Thursday', 'Friday', 'Saturday'][timestamp.getDay()].slice(0,3)",
        "desc": "Short form of day of the week (e.g Wed)"
    },
    {
        "selector": "E",
        "eval": "Math.ceil(((timestamp.getTime()-new Date((new Date).getFullYear(),0,1).getTime())/864e5+new Date((new Date).getFullYear(),0,1).getDay()+1)/7)",
        "desc": "Week of the year (e.g 39)"
    },
    {
        "selector": "M",
        "eval": "timestamp.getMonth() + 1",
        "desc": "Month (e.g 9)"
    },
    {
        "selector": "0M",
        "eval": "('0' + (timestamp.getMonth() + 1)).substr(-2)",
        "desc": "Month (lead 0) (e.g 09)"
    },
    {
        "selector": "N",
        "eval": "['January', 'February',  'March',  'April',  'May',  'June',  'July', 'August',  'September',  'October',  'November',  'December'][timestamp.getMonth()]",
        "desc": "Month (word form) (e.g September)"
    },
    {
        "selector": "n",
        "eval": "['January', 'February',  'March',  'April',  'May',  'June',  'July', 'August',  'September',  'October',  'November',  'December'][timestamp.getMonth()].slice(0,3)",
        "desc": "Month (short word form) (e.g Sep)"
    },
    {
        "selector": "D",
        "eval": "timestamp.getDate()",
        "desc": "Day (e.g 23)"
    },
    {
        "selector": "0D",
        "eval": "('0' + timestamp.getDate()).substr(-2)",
        "desc": "Day (lead 0) (e.g 23)"
    },
    {
        "selector": "H",
        "eval": "timestamp.getHours() % 12 || 12",
        "desc": "Hour (12 hour format) (e.g 7)"
    },
    {
        "selector": "0H",
        "eval": "('0' + (timestamp.getHours() % 12 || 12)).substr(-2)",
        "desc": "Hour (12 hour format) (lead 0) (e.g 07)"
    },
    {
        "selector": "h",
        "eval": "timestamp.getHours()",
        "desc": "Hour (24 hour format) (e.g 13)"
    },
    {
        "selector": "0h",
        "eval": "('0' + timestamp.getHours()).substr(-2)",
        "desc": "Hour (24 hour format) (lead 0) (e.g 13)"
    },
    {
        "selector": "m",
        "eval": "timestamp.getMinutes()",
        "desc": "Minute (e.g 6)"
    },
    {
        "selector": "0m",
        "eval": "('0' + timestamp.getMinutes()).substr(-2)",
        "desc": "Minute (lead 0) (e.g 06)"
    },
    {
        "selector": "s",
        "eval": "timestamp.getSeconds()",
        "desc": "Second (e.g 54)"
    },
    {
        "selector": "0s",
        "eval": "('0' + timestamp.getSeconds()).substr(-2)",
        "desc": "Second (lead 0) (e.g 54)"
    },
    {
        "selector": "a",
        "eval": "timestamp.getHours() < 12 ? 'a' : 'p'",
        "desc": "Time ending in 12 hour formats, without the M (e.g p) (p.s You can use %am for the full ending)"
    },
    {
        "selector": "A",
        "eval": "timestamp.getHours() < 12 ? 'A' : 'P'",
        "desc": "same as %a, but capital (e.g P)"
    }
]
